<h1 style="text-align: center;">Saved Searches</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Location</th>
      <th scope="col">Time</th>
    </tr>
  </thead>
  <tbody>
    <% for searches in @search %>
        <tr>
        <td><%= searches[:location] %></td>
        <td><%= searches[:time] %></td>
        </tr>
    <% end %>
  </tbody>
</table>